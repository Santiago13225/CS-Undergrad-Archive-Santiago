Width?this.articleWidth=document.getElementById("js-mobile-logo").offsetWidth:this.articleWidth=document.getElementById("js-verticals-menu").offsetWidth},getOffsetLeft:function(e){var t=e.getBoundingClientRect(),n=window.pageXOffset||document.documentElement.scrollLeft;return t.left+n},left:function(){if(document.getElementById("secondary")||!document.querySelector(".alignleft"))return!1;var e,t=n(document.querySelectorAll(".alignleft"));try{for(t.s();!(e=t.n()).done;){var r=e.value,i=this.getOffsetLeft(r)-this.offset;"A"===r.parentElement.tagName&&(r=r.parentElement),14<i?r.style.setProperty("margin-left","-".concat(i,"px")):r.style.setProperty("margin-left","0px")}}catch(e){t.e(e)}finally{t.f()}},right:function(){if(document.getElementById("secondary")||!document.querySelector(".alignright"))return!1;var e,t=n(document.querySelectorAll(".alignright"));try{for(t.s();!(e=t.n()).done;){var r=e.value,i=this.windowWidth-(this.getOffsetLeft(r)+r.offsetWidth)-document.getElementById("js-verticals-menu").offsetLeft;"A"===r.parentElement.tagName&&(r=r.parentElement),14<i?r.style.setProperty("margin-right","-".concat(i,"px")):r.style.setProperty("margin-right","0px")}}catch(e){t.e(e)}finally{t.f()}},wide:function(){if(document.getElementById("secondary")||!document.querySelector(".alignwide"))return!1;var e,t=this.getOffsetLeft(document.querySelector(".post-content")),r=n(document.querySelectorAll(".alignwide"));try{for(r.s();!(e=r.n()).done;){var i=e.value,o=this.getOffsetLeft(i)-this.offset,a=t-this.offset;if("A"===i.parentElement.tagName&&(i=i.parentElement),14<o){i.style.setProperty("margin-left","-".concat(o,"px")),i.style.setProperty("width","".concat(this.articleWidth,"px"));for(var s=0;s<i.children.length;s++)i.children[s].classList.contains("wp-caption-text")&&i.children[s].style.setProperty("margin-left","".concat(a,"px"))}else{i.style.setProperty("margin-left","0px"),i.style.setProperty("width","100%");for(s=0;s<i.children.length;s++)i.children[s].classList.conta