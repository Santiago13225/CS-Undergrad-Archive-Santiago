		flipInit: function () {
			// Push ID's into an array that we can access later.
			jQuery('.ui.card[data-flipcard-id]').each(function () {
				var id = jQuery(this).attr('data-flipcard-id');
				// If the set is not already defined then push it to the sets index.
				if (prcShortcodes.cards.flipSets.indexOf(id) === -1) {
					prcShortcodes.cards.flipSets.push(id);
				}
			});

			// Build markup for sides and shape and init shape function
			jQuery.each(prcShortcodes.cards.flipSets, function (index, value) {
				jQuery('.ui.card[data-flipcard-id="' + value + '"]').attr('is-flipcard', true);

				jQuery('.ui.card[data-flipcard-id="' + value + '"]').append('<div class="ui bottom attached button disabled"><i class="undo icon"></i>Click to Flip Over</div>');

				jQuery('.ui.card[data-flipcard-id="' + value + '"]').first().find('.ui.bottom.attached .icon').removeClass('undo').addClass('redo');

				jQuery('.ui.card[data-flipcard-id="' + value + '"]').wrap('<div class="side" data-side="back" data-flipcard-id="' + value + '"></div>');

				jQuery('.ui.card[data-flipcard-id="' + value + '"]').first().parent('.side').addClass('active').attr('data-side', 'front');

				jQuery('.ui.card[data-flipcard-id="' + value + '"]').removeAttr('data-flipcard-id');

				jQuery('<div class="shortcode-flipcard ui shape auto" data-flipcard-id="' + value + '"><div class="sides"></div></div>').insertBefore('.side.active[data-flipcard-id="' + value + '"]');

				jQuery('.side[data-flipcard-id="' + value + '"]').appendTo('.ui.shape[data-flipcard-id="' + value + '"] .sides');

				jQuery('.ui.shape[data-flipcard-id="' + value + '"]').shape();
			});

			// Register click handler.
			jQuery('.shortcode-flipcard .side').click(function () {
				var value = jQuery(this).attr('data-flipcard-id');
				jQuery('.ui.shape[data-flipcard-id="' + value + '"]').shape('flip back');
			});
		},
		collectionInit: function () {
			// Push ID's into an array that we can access later.
			jQuery('.ui.card[data-cards-id]').each(function () {
				var id = jQuery(this).attr('data-cards-id');
				// If the set is not already defined then push it to the sets index.
				if (prcShortcodes.cards.sets.indexOf(id) === -1) {
					prcShortcodes.cards.sets.push(id);
				}
			});

			jQuery.each(prcShortcodes.cards.sets, function (index, value) {
				// Determine if this set of cards is flipcards:
				var flipcards = false;
				if ('true' == jQuery('.ui.card[data-cards-id="' + value + '"]').attr('is-flipcard')) {
					var flipcards = true;
				}

				var total = jQuery('.ui.card[data-cards-id="' + value + '"]').length;
				var target = '.ui.card';
				if (true == flipcards) {
					// Because flipcards come in pairs the total should be adjusted accordingly.
					var total = total / 2;
					var target = '.shortcode-flipcard';
					// Move the target attribute up three levels to .shortcode-flipcard.ui.shape
					jQuery('.ui.card[data-cards-id="' + value + '"]').each(function (index) {
						jQuery(this).removeAttr('data-cards-id');
						jQuery(this).parent().parent().parent().attr('data-cards-id', value);
					});
				}

				var alignment = '';
				if (total > 3) {
					var alignment = 'alignwide';
				}

				jQuery(target + '[data-cards-id="' + value + '"]').first().before('<aside class="shortcode-cards ui stackable equal width grid ' + alignment + '" data-cards-collection="' + value + '"></aside>');

				for (var i = 1; i <= total; i++) {
					jQuery('<div class="column"></div>').prependTo('.ui.stackable.equal.width.grid[data-cards-collection="' + value + '"]');
				}

				jQuery(target + '[data-cards-id="' + value + '"]').each(function (index) {
					index++;
					if (false == flipcards) {
						jQuery(this).addClass('fluid');
					}
					jQuery(this).prependTo('.ui.stackable.equal.width.grid[data-cards-collection="' + value + '"] .column:nth-of-type(' + index + ')');
				});

			});
		}
	},
	tabs: {
		init: function () {
			var tabsElement = document.getElementsByClassName('menu tabular shortcode-tabs-setup');
			var dropdownElement = document.getElementsByClassName('ui dropdown shortcode-tabs-setup');
			var pointingElement = document.getElementsByClassName('menu pointing shortcode-tabs-setup');
			var pillsElement = document.getElementsByClassName('ui pills shortcode-tabs-setup');
			var radioElement = document.getElementsByClassName('ui radio-setup shortcode-tabs-setup');
			if (tabsElement != null) {
				this.tabs(tabsElement);
			};
			if (dropdownElement != null) {
				this.dropdown(dropdownElement);
			};
			if (pointingElement != null) {
				this.pointing(pointingElement);
			};
			if (pillsElement != null) {
				this.pills(pillsElement);
			};
			if (radioElement != null) {
				this.radio(radioElement);
			};
		},
		tabs: function (tabsElement, removeAttached = false) {
			for(var i = 0; i < tabsElement.length; i++){
				var elementName = tabsElement[i];
				
				this.createContainer(elementName);

				var id = elementName.id;
				
				var items = document.querySelector('#' + id + '.menu').children;
				
				this.loopItemsTabs(items, id, elementName);

				var firstItem = document.getElementById(id).getElementsByClassName('item')[0];
				var firstSegment = document.querySelector('#' + id + " .segment");
				firstItem.classList.add('active');
				firstSegment.classList.add('active');

				jQuery('#' + id + ' .menu.shortcode-tabs-setup .item').tab({
					context: 'parent'
				});

				if ( true === removeAttached ) {
					this.removeBottomAttached(id);
				}
			};
		},
		dropdown: function (dropdownElement) {
			for(var i = 0; i < dropdownElement.length; i++){
				var elementName = dropdownElement[i];

				this.createContainer(elementName);

				var items = document.querySelector('#' + id + ' .menu').children;
				
				this.loopItemsTabs(items, id, elementName);

				var firstItem = document.getElementById(id).getElementsByClassName('item')[0];
				var firstSegment = document.querySelector('#' + id + " .segment");
				firstItem.classList.add('active');
				firstSegment.classList.add('active');
				jQuery('#' + id + ' .ui.dropdown.shortcode-tabs-setup').dropdown({
					onChange: function (value, text, $selectedItem) {
						var id = elementName.parentNode.id;
						var active = jQuery($selectedItem).attr("data-tab");
						document.querySelector('#' + id + ' .ui.tab.active').classList.remove('active');
						document.querySelector(".ui.tab[data-tab='" + active + "']").classList.add('active');
						document.querySelector(".ui.dropdown.selection.shortcode-tabs-setup .text").classList.add('default');
					}
				});

				this.removeBottomAttached(id);
			}
		},
		pointing: function (pointingElement) {
			for(var i = 0; i < pointingElement.length; i++){
				var elementName = pointingElement[i];

				this.createContainer(elementName);

				var items = document.querySelector('#' + id + '.menu').children;
				
				this.loopItemsTabs(items, id, elementName);

				var firstItem = document.getElementById(id).getElementsByClassName('item')[0];
				var firstSegment = document.querySelector('#' + id + " .segment");
				firstItem.classList.add('active');
				firstSegment.classList.add('active');

				jQuery('#' + id + ' .menu.shortcode-tabs-setup .item').tab({
					context: 'parent'
				});
			}
		},
		pills: function (pillsElement) {
			this.tabs(pillsElement, true);
		},
		radio: function (radioElement) {
			for(var i = 0; i < radioElement.length; i++){
				var elementName = radioElement[i];

				this.createContainer(elementName);

				var buttons = document.querySelectorAll('#' + id + ' input[type="radio"]');
				buttons[0].defaultChecked = true;
				
				var items = document.querySelectorAll('#' + id + ' .checkbox');
				
				this.loopItemsTabs(items, id, elementName);

				var firstSegment = document.querySelector('#' + id + " .segment");
				firstSegment.classList.add('active');

				jQuery('#' + id + ' .ui.radio-setup.shortcode-tabs-setup .ui.radio.checkbox').checkbox({
					onChange: function (value, text, $selectedItem) {
						var id = elementName.parentNode.id;
						console.log(this);
						var active = this.parentNode.getAttribute('data-tab');
						document.querySelector('#' + id + ' .ui.tab.active').classList.remove('active');
						document.querySelector(".ui.tab[data-tab='" + active + "']").classList.add('active');
					}
				});
				
				this.removeBottomAttached(id);
				
			}
		},
		createContainer: function(elementName) {
			var tabContainer = document.createElement('div');
			tabContainer.setAttribute('id', elementName.id);
			tabContainer.setAttribute('class', 'shortcode-tabs');
			elementName.parentNode.insertBefore(tabContainer, elementName);
		},
		loopItemsTabs: function(items, id, elementName) {
			var tabs = [];
			for(var i = 0; i < items.length; i++){
				var tabID = items[i].getAttribute('data-tab');
				tabs.push(tabID);
			};
			for (var i = 0; i < tabs.length; i++) {
				var segment = document.querySelector('.tab.segment[data-tab="' + tabs[i] + '"]');
				var container = document.getElementById(id);
				container.appendChild(segment);
			};
			elementName.removeAttribute('id');
			container.prepend(elementName);
		},
		removeBottomAttached: function(id) {
			var bottom = document.querySelectorAll('#' + id + " .ui.tab.bottom.attached.segment");
			for (var i = 0; i < bottom.length; i++) {
				bottom[i].classList.add('basic');
				bottom[i].classList.remove('bottom', 'attached');
			}
		}
	}
}

jQuery(document).ready(function () {
	prcShortcodes.init();
});
jQuery(window).load(function () {
	prcShortcodes.gifPlayer.init();
});;
/*! DataTables 1.10.19
