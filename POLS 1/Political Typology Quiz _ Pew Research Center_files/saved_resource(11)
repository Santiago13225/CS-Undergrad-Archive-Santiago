"use strict";var prcShim={init:function(){jQuery('ol.chart_tabs a[href="#share"]').click(function(){prcShim.modifyCharts()}),prcShim.modifyCallouts(),prcShim.modifyBigNumbers()},modifyCharts:function(){jQuery("#share ul").addClass("ui link list"),jQuery("#share .button.twtr-pew-share, #share .button.fb-pew-share").addClass("ui")},modifyCallouts:function(){var t=document.querySelectorAll(".callout"),r=!0,e=!1,i=void 0;try{for(var a,l=t[Symbol.iterator]();!(r=(a=l.next()).done);r=!0){var o=a.value;o.classList.contains("callout-wide")?o.classList.remove("callout-wide"):o.classList.add("alignright"),o.classList.add("ui","inverted","beige","segment","shortcode-callout"),o.classList.remove("callout")}}catch(t){e=!0,i=t}finally{try{r||null==l.return||l.return()}finally{if(e)throw i}}},modifyBigNumbers:function(){var t=document.querySelectorAll(".shortcode-big-number"),r=!0,e=!1,i=void 0;try{for(var a,l=t[Symbol.iterator]();!(r=(a=l.next()).done);r=!0){var o=a.value;"STRONG"===o.parentElement.nodeName&&o.parentElement.classList.add("versal-bold")}}catch(t){e=!0,i=t}finally{try{r||null==l.return||l.return()}finally{if(e)throw i}}}};jQuery(document).ready(function(){prcShim.modifyCallouts()}),jQuery(window).on("load",function(){prcShim.init()});;
